
@{
    ViewBag.Title = "SupplierList";
    Layout = "~/Views/_BlogLayout.cshtml";
}
    <div class="container">
        <div class="row">
            <div class="col-md-12">
                <table class="table table-hover table-bordered">
                    <thead>
                        <tr>
                            <th>firma adı</th>
                            <th>yetkili ünvan</th>
                            <th>yetkili kişi</th>
                            <th>adres</th>
                            <th>telefon</th>
                            <th>şehir/ülke</th>
                        </tr>
                    </thead>
                    <tbody>
                        @foreach (Suppliers supplier in ViewBag.Supplier)
                        {
                            <tr>
                                <td>@supplier.CompanyName</td>
                                <td>@supplier.ContactTitle</td>
                                <td>@supplier.ContactName</td>
                                <td>@supplier.Address</td>
                                <td>@supplier.Phone</td>
                                <td>@supplier.City/@supplier.Country</td>
                            </tr>
                        }
                    </tbody>
                  
                </table>
            </div>
        </div>
    </div>
